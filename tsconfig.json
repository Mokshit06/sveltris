{
  "exclude": [
    "./node_modules",
    "./dist",
    "./frameworks"
  ],
  "compilerOptions": {
    "isolatedModules": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ESNext",
    "strict": false,
    "allowJs": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  }
}